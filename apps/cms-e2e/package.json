{
  "name": "@podcast-system/cms-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@podcast-system/cms"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/cms-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@podcast-system/cms:build",
          "@podcast-system/cms:serve"
        ]
      }
    }
  }
}
