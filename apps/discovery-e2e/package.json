{
  "name": "@podcast-system/discovery-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@podcast-system/discovery"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "discovery-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@podcast-system/discovery:build",
          "@podcast-system/discovery:serve"
        ]
      }
    }
  }
}
